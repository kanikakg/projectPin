<h2><%=@board.name%></h2>
<h4> By: <%= link_to @board.user.username,  user_path(@board.user.username) %></h4>
<div>
<% if @board.user==current_user %>
	<button type="btn-btn default">
		<%=link_to "Edit",edit_board_path(@board)%>
	</button>
	<button type="btn-btn default">
			<%=link_to "Delete", @board, method: :delete , data: { confirm: "Are you sure?" }%>
	</button>
<%end%>
</div>
<div id ="pins" class="transitions-enabled">
<% @board.pins.each do |pin|%>
<div class="box">
			<div class="panel panel-default">
				<%= link_to (image_tag pin.image.url),pin %>
				<h2>
					<%=link_to pin.title,pin %>	</h2>

					<h5>Posted in: <%=link_to pin.board.name ,pin.board %>
					
				</h5>

				<p class="user">
					Submitted By:
					
						<%=link_to pin.user.username, user_path(pin.user.username)%>
					
				</p>
			</div>
		</div>
		<div class="panel-body">
				
		</div>

<%end%>
</div>
