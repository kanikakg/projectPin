<header style="">
<div>
<%= image_tag @user.avatar.url(:medium) %>
</div>
<h2>Username: <%=@user.username%></h2>
<h3>Bio: <%=@user.bio%></h3>
</header>

<h3>My Boards</h3>
<div id ="pins" class="transitions-enabled">
	<% @user.boards.each do |board| %>
		<div class="box">
			<div class="panel panel-default">
				Place image here
				<h2>
					<%=link_to board.name, board%>
				</h2>
				
			</div>
		</div>
		<div class="panel-body">
				
			<%end%>
		</div>
</div>


