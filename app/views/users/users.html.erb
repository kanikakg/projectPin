<ul class="nav nav-tabs nav-justified">

  <li class="active"><a href="/categories" style="margin-left: 130px; padding-left: 15px;">Categories</a></li>
   <li class="active"><a href="/boards">Boards</a></li>
  <li class="active"><a href="/users" style="margin-right: 190px;">Users</a></li>
</ul>

<div class="panel panel-default" style="margin-left: 130px; margin-right: 190px; padding-left: 15px; ">

<% @users.each do |user| %>
<h3>
<%=link_to user.email, user_path(user.username)%> <% if current_user.can_follow user.id %> 
<button><%= link_to "Follow", url_for(controller: :users, action: :follow, followee_id: user.id), method: :post %></button>
<% end %>
<% if current_user.can_un_follow user.id %> 
<button><%= link_to "UnFollow", url_for(controller: :users, action: :un_follow, followee_id: user.id), method: :post %></button>
<% end %>

</h3>


	<%end%>

</div>